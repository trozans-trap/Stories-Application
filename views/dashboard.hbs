<h6>DashBoard</h6>
<h3>Welcome {{name}}</h3>
<p>Here are your stories</p>
{{#if stories}}
    <table class="stripped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Date</th>
                <th>Status</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
           {{#each stories}}
              <tr>
                  <td>
                      <a href="/stories/{{_id}}">{{title}}</a>
                      <td>{{createdAt}}</td>
                      <td><span class="dash-status">{{status}}</span></td>
                      <td></td>
                  </td>
              </tr>  
           {{/each}}
        </tbody>
    </table>
{{else}}
  <p>You have not created any stories</p>
{{/if}}